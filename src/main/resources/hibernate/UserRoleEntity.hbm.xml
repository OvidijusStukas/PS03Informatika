<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="edu.informatika.semestrinis.entity.UserRoleEntity" table="UserRole">

    <id name="id" column="UserRoleId" type="int">
      <generator class="identity"/>
    </id>

    <property name="name" column="UserRoleName" type="string"/>
  </class>
</hibernate-mapping>